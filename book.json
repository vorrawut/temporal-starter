{
  "title": "Temporal Workflow Bootcamp",
  "description": "Master production-ready distributed systems with Temporal workflows",
  "author": "Temporal Workflow Bootcamp Team",
  "language": "en",
  "gitbook": ">=3.0.0",
  "root": ".",
  "structure": {
    "readme": "README.md",
    "summary": "SUMMARY.md"
  },
  "plugins": [
    "github",
    "edit-link",
    "prism",
    "-highlight",
    "copy-code-button",
    "expandable-chapters-small",
    "anchors",
    "back-to-top-button",
    "theme-comscore"
  ],
  "pluginsConfig": {
    "github": {
      "url": "https://github.com/temporal-starter/temporal-workflow-bootcamp"
    },
    "edit-link": {
      "base": "https://github.com/temporal-starter/temporal-workflow-bootcamp/edit/main",
      "label": "Edit This Page"
    },
    "prism": {
      "css": [
        "prismjs/themes/prism-tomorrow.css"
      ]
    },
    "theme-comscore": {
      "showLevel": false
    }
  },
  "variables": {
    "version": "1.0.0",
    "course": "Temporal Workflow Bootcamp"
  }
} 